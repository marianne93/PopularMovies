<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@android:color/black</item>
        <item name="colorPrimaryDark">@android:color/black</item>
        <item name="colorAccent">@android:color/black</item>
    </style>
    <style name="SettingsTheme" parent="AppTheme">
    </style>
    <!--Fonts-->
    <!-- Normal White -->
    <style name="textNormalWhite">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="textSmallWhite" parent="textNormalWhite">
        <item name="android:textSize">@dimen/font_small</item>
    </style>

    <style name="textXMediumWhite" parent="textNormalWhite">
        <item name="android:textSize">@dimen/font_xmedium</item>
    </style>

    <style name="textMediumWhite" parent="textNormalWhite">
        <item name="android:textSize">@dimen/font_medium</item>
    </style>

    <style name="textLargeWhite" parent="textNormalWhite">
        <item name="android:textSize">@dimen/font_large</item>
    </style>

    <style name="textxLargeWhite" parent="textNormalWhite">
